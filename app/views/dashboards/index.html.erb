<% content_for :head, "GalleryProofing"%>
<div>
  <%if user_signed_in?%>
    <!-- top -->
    <% content_for :active_class, "active"%>
    <% content_for :body_class, "dashboard-index"%>

    <!-- main -->
    <!-- top -->
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <!-- title -->
          <div class="page-title dashbord-title">
            <div class="row">
              <div class="col-md-6 col-sm-5 col-xs-5">
              </div>
              <div class="col-md-6 col-sm-7 col-xs-7">
                <div class="btn-group on-page-help-container">
                  <a href="#" class="on-page-help dropdown-toggle" data-toggle="dropdown">
                    <span class="fa fa-question"></span>
                  </a>
                  <ul id="onPageHelp" class="dropdown-menu dropdown-menu-right arrow-upper-right" role="menu">
                    <li><a href="#" target="_blank">Browse Full Knowledge Bas</a></li>
                    <li><a href="#"><span class="email"><i class="fa fa-envelope"></i></span>Email Us</a></li>
                  </ul>
                </div>
                <div class="right-buttons text-right">Help</div>
              </div>
            </div>
          </div>
          <!-- /title -->
        </div>
      </div>
    </div>
    <!-- /top -->

    <!-- main -->
    <div class="modules-container">
      <div class="container-fluid">
        <div class="row">
          <% if @dashboard.show_video == true %>
            <div class="module welcome" id="welcome-video">
              <div class="module-heading">
                Welcome to GalleryProofing
                <%= link_to "Dismiss", "#", class: "js-dismiss-welcome heading-right" %>
                <!-- <a href="#" class="js-dismiss-welcome heading-right">Dismiss</a> -->
              </div>
              <div class="module-body">
                <div data-toggle="modal" data-target="#modal1469790598756" class="video-placeholder">
                  <a href="#" class="js-show-welcome-video">
                    <img src="https://d1icb03h9nte03.cloudfront.net/201607281469727466/studio/v2/img/dashboard/welcome-video.png">
                  </a>
                </div>
                <p>Watch this short video for a quick tour of your new GalleryProofing account!</p>
              </div>
            </div>
          <% end %>
          <% if @dashboard.add_gallery == false || @dashboard.upload_photos == false || @dashboard.setup_colo_logo == false || @dashboard.customize_watermark == false  %>
          <div class="module completeness-factor cf-get-started">
            <div class="module-heading">
              Get Started
            </div>
            <div class="module-body">
              <div class="cf-item">
                <% if @galleries.present? %>
                  <div class="marker checkmark">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 18 19">
                      <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
                        <path d="M1,10.6831489 L7.95234662,16.7914833 L16.79,1" stroke="#3E6978" stroke-width="1" sketch:type="MSShapeGroup"></path>
                      </g>
                    </svg>
                  </div>
                <% else %>
                  <div class="marker">1</div>
                <% end %>
                <div class="cf-details complete">
                  <%= link_to "Add a Gallery", galleries_path, class: "cf-link" %>
                  <p class="cf-desc">Create your first gallery</p>
                </div>
                <div class="clearfix"></div>
              </div>
              <div class="cf-item">
                <% if @photos.count > 0 %>
                  <div class="marker checkmark">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 18 19">
                      <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
                        <path d="M1,10.6831489 L7.95234662,16.7914833 L16.79,1" stroke="#3E6978" stroke-width="1" sketch:type="MSShapeGroup"></path>
                      </g>
                    </svg>
                  </div>
                <% else %>
                  <div class="marker">2</div>
                <% end %>

                <div class="cf-details ">
                  <%= link_to "Upload Photos", galleries_path	 ,class: "cf-link" %>
                  <p class="cf-desc">Add photos to a gallery</p>
                </div>
                <div class="clearfix"></div>
              </div>
              <div class="cf-item">
                <% if @watermark.present? %>
                  <div class="marker checkmark">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 18 19">
                      <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
                        <path d="M1,10.6831489 L7.95234662,16.7914833 L16.79,1" stroke="#3E6978" stroke-width="1" sketch:type="MSShapeGroup"></path>
                      </g>
                    </svg>
                  </div>
                <% else %>
                  <div class="marker">3</div>
                <% end %>

                <div class="cf-details ">
                  <%= link_to "Customize Watermarks", watermarks_path	 ,class: "cf-link" %>
                  <p class="cf-desc">Create image or text based watermarks</p>
                </div>
                <div class="clearfix"></div>
              </div>
              <div class="cf-item">
                <% if @color_logo_present == true %>
                  <div class="marker checkmark">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 18 19">
                      <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
                        <path d="M1,10.6831489 L7.95234662,16.7914833 L16.79,1" stroke="#3E6978" stroke-width="1" sketch:type="MSShapeGroup"></path>
                      </g>
                    </svg>
                  </div>
                <% else %>
                  <div class="marker">4</div>
                <% end %>

                <div class="cf-details ">
                  <%= link_to "Set up Colors and Logos", color_logos_path	 ,class: "cf-link" %>
                  <p class="cf-desc">For galleries, mobile apps, emails and invoices</p>
                </div>
                <div class="clearfix"></div>
              </div>
            </div>
          </div>
          <% end %>
          <div class="module notifications">
            <div class="module-heading">
              Notifications
              <%= link_to "View All", notifications_path,class: "heading-right" %>
            </div>
            <div class="module-body notification">
              <% if @notifications.count > 0 %>
                <% @notifications.each do |notification| %>
                  <div class="notification-item notification-item-style-container" data-dismiss-notification = "<%= notification.id %>">
                    <div class="notification-details">
                      <a href="/v2/103809/order/view/1116269">
                        <div class="top-line-container">
                          <div class="type-and-descriptor-container">
                            <div class="notification-type notification-type-id-1">
                              <% if notification.notification_from == "Order" %>
                                <%= link_to "Order", edit_order_path(notification.id_from) %>
                              <% else %>
                                <a href=<%=edit_invoice_path(notification.id)%>> Invoice
                              <% end %>
                            </div>
                            <div class="notification-descriptor">
                              $<%= notification.amount %>
                            </div>
                          </div>
                          <div class="notification-created">
                            <% if notification.created_at.to_date == Date.current %>
                                <%= notification.created_at.strftime("%I:%M %P") %>
                            <% else %>
                                <%= notification.created_at.strftime("%d-%b-%Y") %>
                            <% end %>
                          </div>
                        </div>
                        <div class="notification-message">
                          <% if notification.notification_from == "Order" %>
                          <%= notification.first_name %> <%= notification.last_name %> placed a new order
                          <% else %>
                          <%= notification.first_name %> <%= notification.last_name %>'s order is waiting for approval
                          <% end %>
                        </div>
                      </a>
                    </div>
                    <div class="notification-dismiss">
                      <button class="dismiss js-dismiss-notification">
                        <span class="fa fa-times" data-notification-id = "<%= notification.id %>" remote = "true"></span>
                      </button>
                    </div>
                  </div>
                <% end %>
              <% else %>
                <div class="module-body">
                  <div class="empty-state">
                    <p>You have no new notifications</p>
                  </div>
                </div>
              <% end %>
            </div>
          </div>
          <% if @galleries.present? %>
            <div class="module recent-gallery-activity">
            <div class="module-heading">
              Recent Gallery Activity
              <%= link_to "View All", recent_gallery_activity_dashboards_path ,class: "heading-right" %>
            </div>
            <div class="module-body">
              <table class="recent-gallery-activity-table">
                <thead>
                  <tr>
                      <th></th>
                      <th class="fav"><div class="activity-type-heading" data-toggle="tooltip" data-placement="top" title="" data-original-title="Visitors with Favorites"><span class="fa fa-star"></span></div>
                      </th>
                      <th class="cart"><div class="activity-type-heading" data-toggle="tooltip" data-placement="top" data-container="body" title="" data-original-title="Visitors with Items in Cart"><span class="fa fa-shopping-cart"></span></div>
                      </th>
                      <th class="order-not-placed"><div class="activity-type-heading" data-toggle="tooltip" data-placement="top" data-container="body" title="" data-original-title="Visitors who have not placed an Order"><span class="fa fa-ban"></span></div>
                      </th>
                      <th class="ordered"><div class="activity-type-heading" data-toggle="tooltip" data-placement="top" data-container="body" title="" data-original-title="Visitors who have placed an Order"><span class="fa fa-money"></span></div>
                      </th>
                  </tr>
                </thead>
                <tbody>
                  <% @gallery.each do |gallery| %>
                    <tr>
                      <td class="recent-gallery-name">
                      <%= link_to galleries_galleryhome_path(id: gallery.id) do%>
                         <%= gallery.name %>
                      <% end %>
                      </td>
                      <td class="recent-gallery-count visitors-with-favorites">
                        <% count = gallery.favorites.count %>
                        <% if count > 0 %>
                          <%= link_to client_mail_favorites_galleries_path(id: gallery.id) do %>
                            <div class="circle-count"><%= count %></div>
                          <% end %>
                        <% else %>
                          <div class="zero-value"></div>
                        <% end %>
                      </td>
                      <td class="recent-gallery-count visitors-with-cart-items">
                        <% count = gallery.carts[1].count %>
                        <% if count > 0 %>
                          <%= link_to view_cart_path(brand: gallery.brand.id, gallery: gallery.id) do %>
                            <div class="circle-count"><%= count %></div>
                          <% end %>
                        <% else %>
                          <div class="zero-value"></div>
                        <% end %>
                      </td>
                      <td class="recent-gallery-count visitors-never-ordered">
                        <% count = gallery.not_placed_order %>
                        <% if count > 0 %>
                            <div class="circle-count"><%= count %></div>
                        <% else %>
                          <div class="zero-value"></div>
                        <% end %>
                      </td>
                      <td class="recent-gallery-count visitors-with-submitted-orders">
                          <% count = gallery.orders.count %>
                          <% if count > 0 %>
                            <%= link_to orders_path do %>
                              <div class="circle-count"><%= count %></div>
                            <% end %>
                          <% else %>
                            <div class="zero-value"></div>
                          <% end %>
                      </td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
          <% end %>
          <div class="module dashboard-message">
            <div class="module-cover">
              <div class="cover-icon">
                <img src="/assets/dashboard-tiles-09_(1).svg">
              </div>
              <p class="cover-title">Invite Your Friends</p>
              <p class="cover-desc">Receive a free month of service after their first paid billing cycles!</p>
              <div class="cover-cta">
                <%= link_to "Refer Now", refer_friends_refer_path, class: "btn btn-default main-cta" %>
              </div>
            </div>
