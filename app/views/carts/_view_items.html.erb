<% if @custom_gallery_layout.pricing_id.present? %>
<%= stylesheet_link_tag "jquery.Jcrop" %>
<%= javascript_include_tag "jquery.Jcrop" %>

<div class="modal fade" id="add-cart-item" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header" id = "items-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
      </div>
      <div class="modal-body">
        <h3 class="model-title-side" id="choose-item-text">choose item</h3>
        <div id = "do_not_delete">
          <div class="item-choose">
            <img src="" id = "cropbox" ><br/>
            <%= link_to "EDIT CROP","#",:id => "edit_crop", :onclick => 'link_crop()' %>
          </div>
          <div id = "items">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<% end %>
