<script>
$("#back_link1").click(function(){
  if ($("#One1").css("display") == "block")
  {
    $("#Two1").css("display","block");
    $("#One1").css("display","none");
    $("#Cover-Modal-Title1").text("Add Cover");
    $("#Modal-Footer-One1").css("display","none");
    $("#Footer-One1").css("display","none");
  }
  else
  {
    if ($("#One1 #changecover").attr('src') == "")
    {
      $("#profile-change").modal('hide');
      $("#album-title-popup").modal('show');
    }
    else
    {
      $("#Two1").css("display","none");
      $("#Cover-Modal-Title1").text("Gallery Cover");
      $("#Modal-Footer-One1").css("display","block");
      $("#Footer-One1").css("display","block");
      $("#One1").css("display","block");
      $('#back_link1').css("display","block");

    }
  }
})

function back_to_add_cover()
{
  $('#back_link2').css("display","none");
  $('#back_link1').css("display","block");
  $("#Two1").css("display","block");
  $("#One1").css("display","none");
  $("#Footer-One1").css("display","none");
  $("#Modal-Footer-One1").css("display","none");
  $("#Cover-Modal-Title1").text("Add Cover");
  $('#album-title-popup').modal('hide');
  $('#back_link1').click(function(){
    back_to_gallery_cover();
  })
}
function back_to_gallery_cover()
{
  $("#Cover-Modal-Title1").text("Gallery Cover");
  $("#One1").css("display","block");
  $("#Footer-One1").css("display","block");
  $("#Modal-Footer-One1").css("display","block");
  $("#Two1").css("display","none");
  $("#back_link1").css("display","block")
  $("#back_link1").click(function()
  {
      back_to_add_cover();
  });
}
$('#Gallery_cover').click(function()
{
  $("#back_link1").css("display","none");
  $("#Cover-Modal-Title1").text("Gallery Cover");
  $("#One1").css("display","block");
  $("#Footer-One1").css("display","block");
  $("#Modal-Footer-One1").css("display","block");
  $("#Two1").css("display","none");
})
</script>
