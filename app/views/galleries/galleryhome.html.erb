<% content_for :head, "GalleryProofing | Gallery view"%>
<% content_for :body_class, "gallery-edit" %>
<% content_for :photo_class, "active"%>
<%#= javascript_include_tag "lightbox-plus-jquery.min.js" %>

<!-- top -->
<div class="container-fluid">
  <div class="row">
    <div class="col-lg-12">
      <!-- title -->
      <div class="page-title">
        <!-- gallery head -->
        <div class="gallery-meta">
          <div class="cover-photo-container clearfix pull-left">
            <div class="cover-photo default  pull-left">
              <%if @find_gallery.gallery_photo_id != nil%>
                <%=image_tag @find_pic.image.url(:small)%>
              <%elsif @find_gallery.cover_url.present?%>
                <%=image_tag @find_gallery.cover_url.url(:small)%>
              <% end %>
            </div>

            <div class="icon-cta" data-toggle="modal" data-target="cover_modal1">
              <% if @find_gallery.cover_url.present? || @find_gallery.gallery_photo_id.present?%>
                <%=link_to "Change<br>Cover".html_safe, "#profile-change" ,'data-toggle': "modal", class: "change-cover", id: "Gallery_cover"%>
              <% else %>
                <%=link_to "Add<br>Cover".html_safe, "#album-title-popup",'data-toggle': "modal", class: "change-cover" %>
              <% end %>
            </div>
          </div>
          <h3 class="gallery-title pull-left">
            <span class="name pull-left"><%=@find_gallery.name%></span>
            <a href="#" data-toggle="modal" data-target="#rename-modal"><span class="fa fa-pencil circle"></span></a>
          </h3>
          <div class="meta-line">
            <div class="status-flags">
              <div class="flag-active color-success-green"><%= @find_gallery.status%></div>
            </div>
            <div class="shoot-date"><%if @find_gallery.shoot_date.present?%><%=@find_gallery.shoot_date.strftime("%d-%b-%Y")%><% end %></div>
            <div class="photos-count">
              <span class="icon-photo"></span>
              <span class=""><%= @photos_count %></span>
            </div>
          </div>
        </div>
        <!-- /gallery head -->
        <div class="gallery-buttons pull-right text-right">
          <%= link_to "Share","#Share",'data-toggle': "modal",class: "btn btn-sm btn-link email-link email-gallery-link"%>
          <% if @find_gallery.contact_id == nil %>
          <!-- <%#= link_to 'Link a contact', galleries_addcontact_path(gallery_id: @find_gallery.id), class: "btn btn-link btn-sm" %> -->
          <%= link_to 'Link a contact', "#link-contact", 'data-toggle': "modal", class: "btn btn-link btn-sm" %>
          <% end %>
          <a href="#" class="btn btn-link btn-sm">Gallery Visitors</a>
          <!-- <a href="#" class="btn btn-link btn-sm show-archived" data-section="albums">Settings</a> -->

          <%= link_to "Client View", view_gallery_url(brand: @current_subdomain,gallery: @find_gallery.id),target: "_blank", class: "btn btn-link btn-sm" %>
          <!-- <%#= link_to "Client View", view_gallery_url(subdomain: @current_subdomain,gallery: @find_gallery.id),target: "_blank", class: "btn btn-link btn-sm" %> -->

          <%=link_to "Settings",edit_custom_gallery_layout_path(@custom_gallery_layout),class: "btn btn-link btn-sm show-archived"%>

          <div class="btn-group on-page-help-container">
            <a href="#" class="on-page-help dropdown-toggle" data-toggle="dropdown">
              <span class="fa fa-question"></span>
            </a>
            <ul id="onPageHelp" class="dropdown-menu dropdown-menu-right arrow-upper-right" role="menu">
              <li><a href="#" target="_blank">Can I download all of my contacts' information?</a></li>
              <li><a href="#" target="_blank">How do I add and edit contacts?</a></li>
              <li><a href="#" target="_blank">How do I view a contact's activity?</a></li>
              <li><a href="#" class="bold" target="_blank">Browse Full Knowledge Base</a></li>
              <li><a href="#" >Email Us</a></li>
            </ul>
          </div>
        </div>

        <div class="clearfix">


        </div>
      </div>
      <!-- /title -->
    </div>
  </div>
</div>
<!-- /top -->
<!-- main -->
<div id="main">
  <div id="bd" class="display-albums">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <!-- sidebar -->
          <%= render "sidebar"%>
    <!-- main content -->
    <div class="main-content">
      <div id="gallery-body"><%= render "allgalleryphotos", all_photos: @all_photos%></div>
    </div>
    <!-- /main content -->

  </div>
</div>
</div>
</div>
</div>
<!-- /main -->

</div>


<!-- Modal -->
<div class="modal" id="rename-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"  data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Rename</h4>
      </div>

      <%= form_for(@gallery, url: {controller: "galleries", action: "update", id: params[:id]}, method: :patch) do |f|%>

      <div class="modal-body">
        <div class="form-group">
          <%=f.text_field :name,value: @find_gallery.name, class: "form-control" %>
        </div>
      </div>
      <div class="modal-footer">
        <%= link_to "Cancel", "#", "data-dismiss": "modal", class: "btn btn-default" %>
        <%= f.submit "Rename", class: "btn btn-sm btn-primary" %>
      </div>
      <% end %>

    </div>
  </div>
</div>
<!-- /mMdal -->

<!-- Add Album Modal -->
<div class="modal fade" id="Add-album" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"  data-backdrop="static" data-keyboard="false" >
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <%=link_to "&times;".html_safe,galleries_galleryhome_path(id: params[:id]),class: "close"%>

        <h4 class="modal-title" id="myModalLabel">Create a New Album </h4>
      </div>
      <%= form_for @album,remote: true do |f| %>
      <div class="modal-body">

        <div class="field">
          <%= f.label :title %><br>
          <%= f.text_field :title , class: "form-control"%>
        </div>
        <% if params[:album_id].present?%>
          <div>
            <%= f.hidden_field :gallery_id, value: params[:id]%>
            <%= f.hidden_field :parent, value: params[:album_id]%>
            <%= f.hidden_field :level, value: @level.level+1%>
          </div>
        <%else%>
          <div>
            <%= f.hidden_field :gallery_id, value: params[:id]%>
            <%= f.hidden_field :parent, value: 0%>
            <%= f.hidden_field :level, value: 0%>
          </div>
        <% end %>
      </div>
      <div class="modal-footer">
        <div class="actions">
          <%=link_to "Cancel",galleries_galleryhome_path(id: params[:id]),class: "btn btn-default"%>
          <%= f.submit "Add Album",id: "Submit", class: "btn btn-primary" %>
        </div>
      </div>
    </div>
    <% end %>
  </div>
</div>
<!-- /mMdal -->

