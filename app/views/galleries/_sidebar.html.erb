        <div data-target="all-album" class="album-text">Favorites<span class="photo-count">(<%= @favorites.count %>)</span></div>
        <div class="clearfix"></div>
        <% end %>
      </li>
      <li class="switch-album can-drop" data-target="hidden">
        <%= link_to client_email_labeled_galleries_path(id: params[:id],tab: "hidden") do %>
        <div class="album-icon">
