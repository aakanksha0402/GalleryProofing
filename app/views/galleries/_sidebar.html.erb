                              <% end %>
                              <% end %>
                              <span>
                                (<span class="js-event-photos-count"><%=child3.photos.count%></span>)
                              </span>
                            </div>
                          </div>
                        </li>
                        <% end %>
                        <% end %>
                      </ul>
                    </li>
                    <% end %>
                    <% end %>
                  </ul>
                </li>
                <% end %>
                <% end %>
              </ul>
            </li>
            <% end %>
          </ul>
        </div>
        <div class="clearfix"></div>
      </div>
