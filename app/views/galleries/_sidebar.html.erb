          <span class="fa fa fa-user"></span>
        </div>
        <div data-target="all-album" class="album-text"><%=@gallery_contact.firstname%>  <%=@gallery_contact.lastname%></div>
        <div class="clearfix"></div>
      </li>
      <% end %>
      <li class="switch-album can-drop" data-target="#">
        <%= link_to client_mail_favorites_galleries_path(id: params[:id]) do %>
