                      <%# else %>
                        <span class="fa fa-folder"></span>
                      <%# end %>
                    </div> -->
                    <div data-target="all-album" class="album-text">
                      <%if child2.password.present?%>
                        <span class="fa fa-lock">
                        <% end %>
                        <%=link_to albums_albumhome_path(gallery_id: params[:id],album_id: child2.id),style: "color: #889EA6;",remote: true do %>
                        <div class="album-icon">
                          <% if @c2.length == 0 %>
                            <span class="fa fa-folder-open"></span>
                          <% else %>
                            <span class="fa fa-folder"></span>
                          <% end %>
                        </div>
                        <%= child2.title  %>
                        <% @photo_count = child2.photos_count %>
                        <% unless @photo_count == 0 %>
                        (<%= @photo_count %>)
                        <% end %>
                        <% end %>

                      </div>
                    </div>

                    <ul class="list-unstyled">
                      <%@c2.each do |child3|%>
                      <%if child3.parent == child2.id%>
                      <div class="clearfix"></div>
                      <li class="switch-album">
                        <div class="inside-title-name">
                          <!-- <div class="album-icon">
                            <span class="fa fa-folder-open"></span>
                          </div> -->
                          <div data-target="all-album" class="album-text">
                            <%if child3.password.present?%>
                            <span class="fa fa-lock">
                              <% end %>
                              <%=link_to albums_albumhome_path(gallery_id: params[:id],album_id: child3.id),style: "color: #889EA6;",remote: true do %>
