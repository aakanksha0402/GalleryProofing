<% content_for :head, "GalleryProofing | New Template"%>
<% content_for :body_class, "contact-index" %>
<% content_for :studio_class, "active"%>

<% if @not_authorized == true %>
  <%= render '/dashboards/no_authorization' %>
<% else %>

<div class="container-fluid">
  <div class="row">
    <div class="col-lg-12">
      <!-- title -->
      <div class="page-title">
        <div class="row">
          <div class="col-md-6 col-sm-5 col-xs-5">
            <h2>
              <div id="pageTitleHeaderText">Email</div>
            </h2>
          </div>
          <div class="col-md-6 col-sm-7 col-xs-7">
            <div class="btn-group on-page-help-container">
              <a href="#" class="on-page-help dropdown-toggle" data-toggle="dropdown">
                <span class="fa fa-question"></span>
              </a>
              <ul id="onPageHelp" class="dropdown-menu dropdown-menu-right arrow-upper-right" role="menu">
                <li><%= link_to "Browse Full Knowledge Base" ,"#", class: "bold", target: "_blank" %></li>
                <li><%= link_to "#", target: "_blank" do %>
                  <span class="email"><i class="fa fa-envelope"></i></span>Email-Us
                  <% end %>
                </li>
              </ul>
            </div>
            <% if @not_authorized_to_add != true %>
              <div class="right-buttons text-right">
                <%= link_to "New Template", "#newtemp-filter", class: "btn btn-primary", "data-toggle": "modal"%>
              </div>
            <% end %>
            </div>
          </div>
        </div>
        <!-- /title -->
      </div>
    </div>
  </div>
  <!-- /top -->

  <!-- main -->
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <ul class="nav nav-pills in-pills">
          <li class="type"><%= link_to "Activity", email_activity_email_templates_path %></li>
          <li class="active"><%= link_to "Templates", "#2a","data-toggle": "tab" %></li>
          <li class="type "><%= link_to "Automations", automation_series_index_path %></li>
        </ul>
      </div>
    </div>
  </div>

  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="tab-content clearfix">
          <div class="tab-pane active" id="2a">
            <!-- with templates -->
            <% if @email_templates.count >= 1 %>
            <div class="container-fluid">
              <div class="row">
                <div class="col-md-2 col-sm-3">
                  <div class="form-group ">
                    <%#= grouped_collection_select "templates", :default_email_template_type_id, @all, :id, :name, :id, :email_type %>
                    <%= select_tag "templates", options_for_select(@all.map{ |all| [all.text_value, all.id, 'data-t': all.text] }), prompt: 'All Templates', class: "input-lg form-control selectpicker", id: "email_template", remote: true %>
                    <%#= select_tag "templates", grouped_options_for_select(@default_email_template_type, selected_key = nil) %>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-12">
                  <% if @params_email_templates.count > 0 %>
                  <div class="table-responsive has-dropdown">
                    <table class="table">
                      <thead>
                        <tr>
                          <th><%= sortable "email_templates.name", "Name" %></th>
                          <th><%= sortable "email_templates.email_subject", "Subject" %></th>
                          <th><%= "Type" %></th>
                          <th><%= "Used in Automation Series" %></th>
                          <th><%= sortable "email_templates.updated_at", "Last Edit date" %><span class="caret"></span></th>
                          <th></th>
                        </tr>
                      </thead>
                      <% @params_email_templates.each do |email_template| %>
                      <tbody>
                        <tr class= 'clickable-row' data-href="<%= edit_email_template_path(email_template) %>">
                          <td><%= email_template.name %></td>
                          <td><%= email_template.email_subject %></td>
                          <td><%= email_template.default_email_template.email_type %></td>
                          <td><%= email_template.automation_series.pluck(:name).join(", ") %></td>
                          <td><%= email_template.updated_at.strftime("%I:%M %p") %></td>
                          <% if email_template.automation_series.present? %>
                          <td class="quick-actions">
                            <%= link_to "Delete", "#Delete-pop-more", "data-toggle": "modal"%>
                          </td>
                          <% else %>
                          <td class="quick-actions">
                            <%= link_to "Delete", "#Delete-pop-more-#{email_template.id}", "data-toggle": "modal"%>
                          </td>
                          <% end %>
                        </tr>

                      </tbody>

                      <!-- Delete not possible when used in automation -->
                      <div class="modal fade" id="Delete-pop-more" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                        <div class="modal-dialog" role="document">
                          <div class="modal-content">
                            <div class="modal-header">
                              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                              <h4 class="modal-title" id="myModalLabel">Confirm Delete</h4>
                            </div>
                            <div class="modal-body">
                              <p><strong><%= email_template.name %> </strong>is currently used in an automation series and may not be deleted.</p>
                            </div>
                            <div class="modal-footer">
                              <%= link_to "Cancel", "#", "data-dismiss": "modal", class: "btn btn-default" %>
                            </div>
                          </div>
                        </div>
                      </div>
                      <!-- Delete not possible when used in automation -->

                      <!-- Delete Template Modal -->
                      <div class="modal fade" id="Delete-pop-more-<%= email_template.id %>" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                        <div class="modal-dialog" role="document">
                          <div class="modal-content">
                            <div class="modal-header">
                              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                              <h4 class="modal-title" id="myModalLabel">Confirm Delete</h4>
                            </div>
                            <div class="modal-body">
                              <p>Are you sure you want to delete <strong><%= email_template.name %></strong> You will no longer be able to use this email template. This cannot be undone.</p>
                            </div>
                            <div class="modal-footer">
                              <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                              <%= link_to "Yes, Delete", email_template_path(email_template), method: :delete, class: "btn btn-sm btn-danger"%>
                            </div>
                          </div>
                        </div>
                      </div>
                      <!-- Delete Template Modal -->
                      <% end %>
                    </table>
                  </div>
                  <% else %>
                  <%= "No Records Found" %>
                  <% end %>
                </div>
              </div>
            </div>
            <% if params[:automation_series] %>
              <script>
                $(document).ready(function(){
                  $("#email_template").val("<%= params[:automation_series]%>");
                });
              </script>
            <% elsif params[:email_template] %>
              <script>
                $(document).ready(function(){
                  $("#email_template").val("<%= params[:email_template]%>");
                });
              </script>
            <% end %>
            <!-- with templates -->
            <% else %>
            <!-- no templates -->
            <div class="container-fluid">
              <div class="row">
                <div class="col-lg-12 text-center">
