<div class="client-page constrained-width" id="pageHome">

    <style type="text/css">
        html {
            opacity: 0;
        }

        sp-logo {
            width: 120px;
            height: auto;
            max-height: 50px;
        }

        .brand-logo-small {
            display: block;
        }

        .brand-logo-medium,
        .brand-logo-large {
            display: none;
        }

        @media (min-width: 520px) {
            sp-logo {
                width: 160px;
                max-height: 80px;
            }

            .brand-logo-medium {
                display: block;
            }

            .brand-logo-small,
            .brand-logo-large {
                display: none;
            }
        }
        @media (min-width: 1420px) {
            sp-logo {
                width: 200px;
                max-height: 120px;
            }

            .brand-logo-large {
                display: block;
            }

            .brand-logo-small,
            .brand-logo-medium {
                display: none;
            }
        }

        sp-logo img {
            max-width: 100%;
            max-height: 100%;
        }
    </style>

    <script type="text/javascript">
        document.addEventListener("DOMContentLoaded", function (event) {

            // handle logo sizing
            if ($('.brand-logo-small img').length) {
                var width = $('.brand-logo-small img')[0].naturalWidth;
                var height = $('.brand-logo-small img')[0].naturalHeight;

                if (width && height) {

                    if ( ( width / height ) < ( 120 / 50 ) ) {
                        // uploaded image is taller than small container
                        $('.brand-logo-small').height('50px');
                    }
                    if ( ( width / height ) < ( 160 / 80 ) ) {
                        // uploaded image is taller than small container
                        $('.brand-logo-medium').height('80px');
                    }
                    if ( ( width / height ) < ( 200 / 120 ) ) {
                        // uploaded image is taller than small container
                        $('.brand-logo-large').height('120px');
                    }

                }
            }

            // handle attaching theme related classes to <html>
            $('html')
                .addClass("theme-light theme-font-option-1")
                .css('opacity', 1);

        });
    </script>

